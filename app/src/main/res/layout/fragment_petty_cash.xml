<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PettyCash">


    <Button
        android:id="@+id/button_add_cash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/add_cash"
        app:layout_constraintBottom_toBottomOf="@id/button_draw_cash"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_draw_cash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/draw_cash"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/button_add_cash" />


</androidx.constraintlayout.widget.ConstraintLayout>